{
  "version": 2,
  "env": {
    "SUPABASE_URL": "SUPABASE_URL",
    "SUPABASE_SERVICE_KEY": "SUPABASE_SERVICE_KEY"
  },
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}